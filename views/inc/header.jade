header.header__Site
    .header__Site--Container
      figure.header__Site--Iz
        h1.title Zimba
      nav.header__Site--De
        ul.ul
         if menu === 'Inicio' 
          li.list.selectItem
            a(href="/panel/", class="tolink")
              i(class="fa fa-home" aria-hidden="true")
              | Inicio
         else
          li.list
            a(href="/panel/", class="tolink")
              i(class="fa fa-home" aria-hidden="true")
              | Inicio
         if menu === 'Propiedades' 
          li.list.selectItem
            a(href="/panel/propiedades", class="tolink")
              i(class="fa fa-folder-open" aria-hidden="true")
              | Propiedades
         else
          li.list
            a(href="/panel/propiedades", class="tolink")
              i(class="fa fa-folder-o" aria-hidden="true")
              | Propiedades
         if menu === 'Disponibles'
          li.list.selectItem
            a(href="/panel/disponibles", class="tolink")
             i(class="fa fa-folder-open" aria-hidden="true")
             | Disponibles
         else
          li.list
            a(href="/panel/disponibles", class="tolink")
             i(class="fa fa-folder-o" aria-hidden="true")
             | Disponibles
         if menu === 'Agenda'
          li.list.selectItem
            a(href="/panel/agenda", class="tolink")
             i(class="fa fa-calendar-plus-o", aria-hidden="true")
             | Agenda
         else
          li.list
            a(href="/panel/agenda", class="tolink")
             i(class="fa fa-calendar-plus-o", aria-hidden="true")
             | Agenda

         if menu === 'Config' 
          li.list.selectItem
            a(href="/panel/config", class="tolink")
              i(class="fa fa-cog" aria-hidden="true")
              | Config
         else
          li.list
            a(href="panel/config", class="tolink")
              i(class="fa fa-cog" aria-hidden="true")
              | Config

        div.header__User
          figure.header__UserFigure.jsDownShow
             img(src="/registers/#{foto}", alt="#{nombre}")
             //else
              img(src="http://landestate.com.ar/wp-content/uploads/2014/09/6262664_orig.png")
          div.header__ShowData
            span.jsDownShow
              | #{nombre}
            .header__ShowData--SubMenu
              ul.ul.jsDown.uNoneDisplay
                li.listSub
                  a.link(href="/panel")
                    |Panel
                li.listSub
                  a.link(href="/logout")
                    | Salir
            .header__ShowData--SubMenu--Notify
              ul.ul.jsDownNotify.uNoneDisplay
               each propiedades in serv
                - var data = new Date();
                - var year = data.getFullYear();
                - var month = (data.getMonth() + 1);
                - var monthSum =  ''
                if propiedades.datos.yearexpired == year
                 
                 if month == 1 
                  - var month_tree = '0' + month;
                  if propiedades.datos.monthexpired == month
                    li.listSub.activeNewNotify
                     i.fa.fa-eye(aria-hidden="true")
                      | Esta propiedad ya vencio: #{propiedades.nombrePropiedad}
                 if (month + 1) > 0 && month <= 9
                  - var month_one = '0' + (month + 1); 
                   if propiedades.datos.monthexpired == month_one
                    li.listSub.activeNewNotify
                     i.fa.fa-eye(aria-hidden="true")
                     | Vence el proximo mes: #{propiedades.nombrePropiedad}
                 if (month + 1) > 9
                  - var month_two = (month + 1);
                  if propiedades.datos.monthexpired == month_two
                   li.listSub.activeNewNotify
                    i.fa.fa-eye(aria-hidden="true")
                    | Vence el proximo mes: #{propiedades.nombrePropiedad}
                else
                  //

               
            span.header__ShowNotify.jsShowNotify
              i.fa.fa-bell(aria-hidden="true")


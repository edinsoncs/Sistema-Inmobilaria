        fieldset.fieldset--Form.tree
         input(type="text", placeholder="Fecha de ingreso", name="ingresoinquilino")
      div.fieldset--Steps
        header.header--Steps
          h2.title
            | Inquilino
        div.form--Container
         fieldset.fieldset--Form
           input(type="text", placeholder="Nombre Inquilino", required, name="nameinquilino")
         fieldset.fieldset--Form
           input(type="tel", placeholder="Tel Inquilino", required, name="telinquilino")
        div.form--Container
          fieldset.fieldset--Form
            input(type="number", placeholder="Dni Inquilino", required, name="dniinquilino")
          fieldset.fieldset--Form
            input(type="email", placeholder="Email Inquilino", required, name="emailinquilino")
      div.fieldset--Steps
        header.header--Steps
          h2.title
            | Garante
        div.form--Container
          fieldset.fieldset--Form
            input(type="text", placeholder="Nombre Garante", name="garanteNombre")
          fieldset.fieldset--Form
            input(type="tel", placeholder="Tel Garante", name="garanteTel")
        div.form--Container
          fieldset.fieldset--Form
            input(type="text", placeholder="Dni Garante", name="garanteDni")
          fieldset.fieldset--Form
            input(type="text", placeholder="Email Garante", name="garanteEmail")
        fieldset.fieldset--Form
          input(type="file", name="garanteFile")
      div.fieldset--Steps
        header.header--Steps
          h2.title
            | Contrato
        fieldset.fieldset--Form
          input(type="file", name="contrato")

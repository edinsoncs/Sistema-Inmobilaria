body
  header.header__Site
    .header__Site--Container
      figure.header__Site--Iz
        h1.title Zimba
      nav.header__Site--De
        ul.ul
          li.list
            a.tolink(href="../panel")
              i(class="fa fa-home" aria-hidden="true")
              | Inicio
          li.list.selectItem
            a(href="panel/propiedades", class="tolink")
              i(class="fa fa-folder-open" aria-hidden="true")
              | Propiedades
          //li.list
            i(class="fa fa-users" aria-hidden="true")
            | Miembros
        div.header__User
          figure.header__UserFigure
            img(src="../../images/avatar.jpg", alt="#{nombre}")
          div.header__ShowData.jsDownShow
            span
              | #{nombre}
            .header__ShowData--SubMenu
              ul.ul.jsDown.uNoneDisplay
                li.listSub
                  a.link(href="panel")
                    |Panel
                li.listSub
                  a.link(href="panel/edit")
                    |Editar mi perfil
                li.listSub
                  a.link(href="#")
                    | Salir
  section.header__Welcome.widthBase
    header.header__Welcome--Iz
     ul.ul
      li.list
       a.link(href="#")
        i(class="fa fa-briefcase", aria-hidden="true") 
        |  #{empresa} 
      li.list
       span.break /
      li.list
       a.link.actuality(href="../propiedades")
        | Propiedades
      li.list
       span.break /
      li.list
       a.link.actuality(href="#")
        | Nueva Propiedad

  main.widthBase.add
    header.newPropiedad--Header
      h2.title
        | Crear Nueva Propiedad
    form.newPropiedad--Form(action="../../panel/addcreate", method="POST", enctype="multipart/form-data")
      //div.fieldset--Form
        figure.fieldset--Form--Figure(style="background-image:url('http://images.depto9.com/2014/05/deco-departamentos-modernos-1b.jpg')")
        label(for="thisUploadImage", class="subirImg")
            | Elegir Imagen
      div.fieldset--Steps
       header.header--Steps
        h2.title
          | Propiedad
       //fieldset.fieldset--Form.uNoneDisplay
         input(type="file", required, name="img", id="thisUploadImage")
       fieldset.fieldset--Form
         input(type="text", placeholder="Nombre de la propiedad", required, name="nombrepropiedad")
       fieldset.fieldset--Form
        input(type="text", placeholder="Descripcciòn de la propiedad", required, name="despropiedad")
       div.form--Container
        fieldset.fieldset--Form.tree
          input(type="number", placeholder="$Precio", required, name="pricepropiedad")
        fieldset.fieldset--Form.tree
          select(name="barriopropiedad")
            option(value="Abasto")
              | Abasto
            option(value="Agronomia")
              | Agronomia
